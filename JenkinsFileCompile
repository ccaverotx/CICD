pipeline {
    agent any
    tools {
        maven "MVN_LTS"
    }

    stages {
        stage('Github') {
            steps {
                git branch: 'main', url: 'https://github.com/ccaverotx/CICD.git'
            }
        }
        stage('Clean Compile'){
            steps{
                bat 'mvn clean compile'    
            }    
        }
        stage('Package'){
            steps{
                bat 'mvn package'    
            }    
        }
    }
}
